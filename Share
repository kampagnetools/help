<script>
(function () {
  try {
    if (sessionStorage.getItem('formSubmitFlag') === 'true') return;
    sessionStorage.setItem('formSubmitFlag', 'true');
  } catch (e) {
    if (window.formSubmitFlag) return;
    window.formSubmitFlag = true;
  }
  window.dataLayer = window.dataLayer || [];
  window.dataLayer.push({ event: 'form_submit_once' });
})();
</script>
