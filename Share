<script>
(function () {
  try {
    if (sessionStorage.getItem('formStartFlag') === 'true') return;
    sessionStorage.setItem('formStartFlag', 'true');
  } catch (e) {
    if (window.formStartFlag) return;
    window.formStartFlag = true;
  }
  window.dataLayer = window.dataLayer || [];
  window.dataLayer.push({ event: 'form_start_once' });
})();
</script>
